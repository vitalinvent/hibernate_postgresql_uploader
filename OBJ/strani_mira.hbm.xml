      <property name = "kod_alfa2" column = "kod_alfa2" type = "string"/>
      <property name = "kod_alfa3" column = "salary" type = "string"/>
      <property name = "naimenovanie" column = "naimenovanie" type = "string"/>
      <property name = "naimenovanie_polnoe" column = "naimenovanie_polnoe" type = "string"/>
      <property name = "uchastnik_e_a_e_s" column = "uchastnik_e_a_e_s" type = "boolean"/>
   </class>
</hibernate-mapping>